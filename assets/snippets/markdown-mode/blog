+++
comments = true
date = "`(format-time-string "%Y-%m-%dT%H:%M:%S")+09:00`"
draft = false
image = ""
menu = ""
share = true
slug = "${1:slug}"
categories = ["${2:category}"]
tags = []
title = "${3:title}"

+++
$0
